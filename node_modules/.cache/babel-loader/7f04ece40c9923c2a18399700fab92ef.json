{"ast":null,"code":"import { reactive, ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport { useStore } from \"vuex\";\nexport default {\n  name: 'Login',\n\n  setup() {\n    const formRef = ref(null);\n    const router = useRouter();\n    const store = useStore();\n    const form = reactive({\n      userName: '王小明',\n      userType: '',\n      password: ''\n    });\n    const rules = reactive({\n      userType: [{\n        required: true,\n        message: '請填寫使用者身份',\n        trigger: ['blur', 'change']\n      }],\n      userName: [{\n        required: true,\n        message: '請填寫使用者名稱',\n        trigger: ['blur', 'change']\n      }],\n      password: [{\n        required: true,\n        message: '請填寫密碼',\n        trigger: ['blur', 'change']\n      }]\n    });\n\n    const submit = () => {\n      formRef.value.validate(async valid => {\n        if (!valid) return;\n        form.isLogin = true;\n        await store.dispatch('EDIT_USER_INFO', form);\n        router.push({\n          path: '/'\n        });\n      });\n    };\n\n    return {\n      form,\n      rules,\n      formRef,\n      submit\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAwBA,SAASA,QAAT,EAAmBC,GAAnB,QAA8B,KAA9B;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,SAASC,QAAT,QAAyB,MAAzB;AAEA,eAAe;AACXC,MAAI,EAAE,OADK;;AAEXC,OAAK,GAAG;AACJ,UAAMC,OAAM,GAAIL,GAAG,CAAC,IAAD,CAAnB;AACA,UAAMM,MAAK,GAAIL,SAAS,EAAxB;AACA,UAAMM,KAAI,GAAIL,QAAQ,EAAtB;AACA,UAAMM,IAAG,GAAIT,QAAQ,CAAC;AAClBU,cAAQ,EAAE,KADQ;AAElBC,cAAQ,EAAC,EAFS;AAGlBC,cAAQ,EAAE;AAHQ,KAAD,CAArB;AAKA,UAAMC,KAAI,GAAIb,QAAQ,CAAC;AACnBW,cAAQ,EAAE,CAAC;AAAEG,gBAAQ,EAAE,IAAZ;AAAkBC,eAAO,EAAE,UAA3B;AAAuCC,eAAO,EAAE,CAAC,MAAD,EAAS,QAAT;AAAhD,OAAD,CADS;AAEnBN,cAAQ,EAAE,CAAC;AAAEI,gBAAQ,EAAE,IAAZ;AAAkBC,eAAO,EAAE,UAA3B;AAAuCC,eAAO,EAAE,CAAC,MAAD,EAAS,QAAT;AAAhD,OAAD,CAFS;AAGnBJ,cAAQ,EAAE,CAAC;AAAEE,gBAAQ,EAAE,IAAZ;AAAkBC,eAAO,EAAE,OAA3B;AAAoCC,eAAO,EAAE,CAAC,MAAD,EAAS,QAAT;AAA7C,OAAD;AAHS,KAAD,CAAtB;;AAMA,UAAMC,MAAK,GAAI,MAAM;AACjBX,aAAO,CAACY,KAAR,