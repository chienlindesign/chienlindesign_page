n        const store = useStore();\n        const form = reactive({\n            userName: '王小明',\n            userType:'',\n            password: '',\n        });\n        const rules = reactive({\n            userType: [{ required: true, message: '請填寫使用者身份', trigger: ['blur', 'change'] }],\n            userName: [{ required: true, message: '請填寫使用者名稱', trigger: ['blur', 'change'] }],\n            password: [{ required: true, message: '請填寫密碼', trigger: ['blur', 'change'] }]\n        });\n\n        const submit = () => {\n            formRef.value.validate(async valid => {\n                if (!valid) return;\n                form.isLogin = true;\n                await store.dispatch('EDIT_USER_INFO', form);\n                router.push({\n                    path: '/'\n                });\n            });\n        };\n        return {\n            form,\n            rules,\n            formRef,\n            submit,\n        };\n    }\n};\n</script>\n<style scoped>\n#card {\n  align-items: center;\n  justify-content: center;\n  display: flex;\n  border-radius: 8px;\n  box-shadow: 1px 2px 8px rgba(180, 180, 180, 0.65);\n  height: 400px;\n  margin: 6rem auto 4rem auto;\n  width: 329px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}